{
  "name": "sprout-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "electron": "tsc && cp src/preload.js build/preload.js && cross-env NODE_ENV=development electron .",
    "dev": "concurrently \"npm run start\" \"npm run electron\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/styled-components": "^5.1.34",
    "chia-datalayer": "^2.0.13",
    "chia-datalayer-fs-deploy": "^1.0.15",
    "chia-wallet": "^1.0.15",
    "chia-web2-gateway": "^1.0.10",
    "components": "^0.1.0",
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "flowbite-typography": "^1.0.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.2",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-webview": "^0.1.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.8",
    "xterm": "^5.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "wait-on": "^7.2.0"
  }
}
